{
  "source": {
    "include": ["./javascript/"],
    "includePattern": "\\.(js)$",
    "excludePattern": "node_modules/"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "markdown": {
    "parser": "gfm"
  }
}
